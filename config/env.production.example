# 七牛云API（必需）
QINIU_API_KEY=your_qiniu_api_key_here

# DashScope（阿里云百炼）API（必需：用于故事大纲/剧本/分镜生成；也可用于通义万相文生图）
DASHSCOPE_API_KEY=your_dashscope_api_key_here

# （可选）DashScope 文本生成超时配置（毫秒）
# DASHSCOPE_TIMEOUT_MS=60000
# DASHSCOPE_OUTLINE_TIMEOUT_MS=45000
# DASHSCOPE_SCRIPT_TIMEOUT_MS=90000
# DASHSCOPE_STORYBOARD_TIMEOUT_MS=120000
# DASHSCOPE_CHAT_TIMEOUT_MS=60000

# （可选/历史兼容）DeepSeek 官网直连 Key：当前版本已默认不再使用
# DEEPSEEK_API_KEY=your_deepseek_api_key_here

# 可选：内部 API Key（开启 ENABLE_API_KEY_VERIFICATION 时校验）
# API_KEY=your_internal_api_key_here

# 生产域名白名单（逗号分隔，例如 example.com,api.example.com）
# ALLOWED_ORIGINS=example.com,api.example.com

NODE_ENV=production

